<UserControl x:Class="Sim.Pdf.View.vPDFBrowser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Sim.Pdf.View"
             xmlns:wb="clr-namespace:Sim.Pdf.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <WebBrowser wb:WebBrowserUtility.BindableSource="{Binding ShowPDF, Mode=TwoWay}"/>
        <!--<WebBrowser Source="http://www.jau.sp.gov.br/painel/arquivos/jornal/Jornal_Oficial_740.pdf" Visibility="Collapsed"/>-->
    </Grid>
</UserControl>
